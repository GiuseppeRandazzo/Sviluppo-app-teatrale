  "acts": [
    {
      "id": "9e9d69a6-4964-421b-924c-569392616261",
      "title": "Atto I",
      "description": null,
      "scenes": [
        {
          "id": "b2d68da3-6d4d-46de-b5fa-6dd1f6350155",
          "title": "Scena I",
          "description": null,
          "dialogues": [
